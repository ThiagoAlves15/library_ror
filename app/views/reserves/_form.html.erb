<%= bootstrap_form_with(model: reserve) do |f| %>
  <%= render "layouts/error_messages", resource: reserve %>

  <%= f.text_field :user_id, placeholder: "User id" %>
  <%= f.text_field :book_id, placeholder: "Book id" %>
  <%= f.date_select :devolution_date, placeholder: "Devolution date" %>
  <%= f.submit class: "btn btn-primary btn-lg w-100" %>
<% end %>
